{
  "name": "useful",
  "version": "0.0.1",
  "description": "A set of useful apps",
  "scripts": {
    "open:src": "babel-node tools/serverDev.js",
    "open:docs": "babel-node tools/serverDocs.js",
    "build-docs": "rimraf docs && jsdoc -c jsDoc.json",
    "docs": "npm run build-docs && npm run open:docs",
    "lint": "esw --ext .js --ext .jsx webpack.config.* src tools",
    "test": "jest --coverage",
    "check": "npm-run-all test lint",
    "check:watch": "npm-run-all lint:watch test:watch",
    "lint:watch": "npm run lint -- --watch",
    "test:watch": "npm run test -- --watchAll",
    "start": "npm-run-all --parallel open:src lint:watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm-run-all clean-dist",
    "build": "babel-node tools/build.js",
    "prod": "npm run build && babel-node tools/serverProd.js",
    "ultimate": "npm install && npm run build"
  },
  "license": "ISC",
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~10.0.1",
    "babel-helper-builder-react-jsx": "~6.26.0",
    "babel-helper-regex": "~6.26.0",
    "babel-loader": "~7.1.2",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-preset-stage-1": "~6.24.1",
    "babel-register": "~6.26.0",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "~0.28.11",
    "eslint": "^5.6.0",
    "eslint-config-react-app": "^3.0.3",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-react-hooks": "^1.2.0",
    "eslint-watch": "^4.0.2",
    "express": "~4.16.3",
    "extract-text-webpack-plugin": "~4.0.0-beta.0",
    "file-loader": "~2.0.0",
    "html-webpack-plugin": "~3.2.0",
    "less": "~3.9.0",
    "less-loader": "~4.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prop-types": "~15.6.0",
    "react-svg-loader": "^2.1.0",
    "rimraf": "^2.6.3",
    "style-loader": "~0.19.1",
    "url-loader": "^1.1.2",
    "webpack": "~4.26.1",
    "webpack-dev-server": "~3.1.10"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "i18next": "^15.0.5",
    "i18next-xhr-backend": "^2.0.1",
    "lodash": "^4.17.14",
    "prop-types": "~15.6.2",
    "react": "^16.8.3",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "^16.8.3",
    "react-i18next": "^10.2.1",
    "react-router-dom": "~4.2.2",
    "styled-components": "~4.1.3"
  }
}
