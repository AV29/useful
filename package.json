{
  "name": "useful",
  "version": "0.0.1",
  "description": "A set of useful apps",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --open",
    "lint": "esw --ext .jsx,.js webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "npm run lint",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm run clean-dist",
    "build": "cross-env NODE_ENV=production webpack",
    "prod": "npm run build && babel-node tools/server.prod.js"
  },
  "license": "ISC",
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "devDependencies": {
